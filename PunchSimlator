    Name = "Mo Trung Nhanh"
})
    _G.autoHatch = true
    _G.selectEgg = "1"

    function autoHatch()
        while _G.autoHatch == true do
game:GetService("ReplicatedStorage").Events.PlayerPressedKeyOnEgg:FireServer(_G.selectEgg,1)
            wait(0)
        end
    end

TabTwo:AddToggle({
Name = "Auto Hatch",
    Default = false,
    Callback = function(Value)
                _G.autoHatch = Value
                    autoHatch()
                    end
        })

